{
  "version": 4,
  "configurations": [
    {
      "name": "gcc",
      "intelliSenseMode": "windows-gcc-arm",
      "compilerPath": "${env:MBED_GCC_ARM_PATH}/arm-none-eabi-g++.exe",
      "cStandard": "gnu17",
      "cppStandard": "gnu++17",
      "forcedInclude": [
      ],
      "includePath": [
        "${workspaceRoot}",
        "mbed-os/**"
      ],
      "defines": [
        "USE_HAL_DRIVER",
        "STM32L475xx",
        "DEBUG",
        "LED1",
        "TARGET_DISCO_L475VG_IOT01A"
      ],
      "browse": {
        "path": [
          "${workspaceRoot}"
        ],
        "limitSymbolsToIncludedHeaders": false,
        "databaseFilename": ""
      }
    }
  ]
}